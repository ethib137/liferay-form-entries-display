{
	"$schema": "https://raw.githubusercontent.com/liferay/liferay-js-toolkit/master/resources/schemas/configuration.schema.json",
	"system": {
		"category": "liferay-form-entries-display",
		"name": "liferay-form-entries-display",
		"fields": {
			"email": {
				"type": "string",
				"name": "email",
				"description": "authorized-users-email-address",
				"default": "test@liferay.com"
			},
			"password": {
				"type": "string",
				"name": "password",
				"description": "authorized-users-email-password",
				"default": "test"
			}
		}
	},
	"portletInstance": {
		"fields": {
			"formId": {
				"type": "string",
				"name": "form-id",
				"description": "include-a-form-id-to-display-the-form-results"
			},
			"showDraft": {
				"type": "boolean",
				"name": "show-draft",
				"description": "show-draft-form-entries"
			}
		}
	}
}